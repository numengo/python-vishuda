{
  "$id": "https://numengo.org/vishuda#",
  "type": "object",
  "$comment": "automatically generated for vishuda",
  "$defs": {
    "i18n": {
      "type": "object",
      "$defs": {
        "TranslatableMixin": {
          "type": "object",
          "properties": {
            "multilingual": {
              "type": "array"
            }
          }
        }
      }
    },
    "auth": {
      "type": "object",
      "$defs": {
        "ContentType": {
          "type": "object",
          "extends": [
            "https://numengo.org/ngoschema#/$defs/instances/$defs/Entity"
          ],
          "properties": {
            "app_label": {
              "type": "string"
            },
            "name": {
              "type": "string"
            }
          }
        },
        "Permission": {
          "type": "object",
          "extends": [
            "https://numengo.org/ngoschema#/$defs/instances/$defs/Entity"
          ],
          "properties": {
            "name": {
              "type": "string"
            },
            "codename": {
              "type": "string"
            },
            "content_type": {
              "$ref": "#/$defs/auth/$defs/ContentType"
            }
          }
        },
        "Group": {
          "type": "object",
          "extends": [
            "https://numengo.org/ngoschema#/$defs/instances/$defs/Entity"
          ],
          "properties": {
            "name": {
              "type": "string"
            },
            "permissions": {
              "type": "array",
              "items": {
                "$ref": "#/$defs/auth/$defs/Permission"
              }
            }
          }
        },
        "Address": {
          "type": "object",
          "aliases": {
            "street": "street-address",
            "city": "locality",
            "zip": "postal-code"
          },
          "extends": [
            "https://json-schema.org/address"
          ]
        },
        "Card": {
          "type": "object",
          "aliases": {
            "address": "adr"
          },
          "extends": [
            "https://json-schema.org/card"
          ],
          "properties": {
            "adr": {
              "$ref": "#/$defs/auth/$defs/Address"
            }
          }
        },
        "User": {
          "type": "object",
          "aliases": {
            "firstName": "givenName",
            "fullName": "fn",
            "lastName": "familyName"
          },
          "primaryKeys": [
            "username"
          ],
          "extends": [
            "https://numengo.org/ngoschema#/$defs/instances/$defs/Entity",
            "#/$defs/auth/$defs/Card"
          ],
          "properties": {
            "email": {
              "type": "string"
            },
            "username": {
              "type": "string",
              "default": "{{this.email}}"
            },
            "githubUsername": {
              "type": "string"
            },
            "fn": {
              "type": "string",
              "default": "{{this.firstName}} {{this.lastName}}"
            }
          }
        },
        "BankAccount": {
          "type": "object",
          "aliases": {
            "BIC": "swift_bic",
            "IBAN": "iban"
          },
          "properties": {
            "holder_name": {
              "type": "string"
            },
            "bank_name": {
              "type": "string"
            },
            "bank_street": {
              "type": "string"
            },
            "bank_city": {
              "type": "string"
            },
            "bank_zip": {
              "type": "string"
            },
            "agency": {
              "type": "string"
            },
            "swift_bic": {
              "type": "string"
            },
            "iban": {
              "type": "string"
            }
          }
        }
      }
    },
    "personas": {
      "type": "object",
      "$defs": {
        "Merchant": {
          "type": "object",
          "aliases": {
            "reg_capital": "registration_capital",
            "VAT_id": "merchant_vat_id",
            "reg_city": "registration_city",
            "street": "merchant_street",
            "tax_id": "merchant_tax_id",
            "city": "merchant_city",
            "zip": "merchant_zip",
            "reg_id": "registration_id"
          },
          "primaryKeys": [
            "merchant_name"
          ],
          "extends": [
            "https://numengo.org/django-cms#/$defs/Merchant",
            "#/$defs/auth/$defs/Card"
          ],
          "properties": {
            "bank_account": {
              "$ref": "#/$defs/auth/$defs/BankAccount"
            }
          }
        },
        "Manufacturer": {
          "type": "object",
          "primaryKeys": [
            "fn"
          ],
          "extends": [
            "https://numengo.org/ngoschema#/$defs/instances/$defs/Entity",
            "#/$defs/auth/$defs/Card"
          ]
        },
        "Customer": {
          "type": "object",
          "primaryKeys": [
            "username"
          ],
          "extends": [
            "#/$defs/auth/$defs/User"
          ]
        },
        "BusinessCustomer": {
          "type": "object",
          "primaryKeys": [
            "username"
          ],
          "extends": [
            "#/$defs/personas/$defs/Customer",
            "#/$defs/personas/$defs/Merchant"
          ]
        }
      }
    },
    "app": {
      "type": "object",
      "$defs": {
        "Model": {
          "type": "object",
          "abstract": true,
          "primaryKeys": [
            "canonicalName"
          ],
          "extends": [
            "https://numengo.org/ngoschema#/$defs/instances/$defs/Entity",
            "#/$defs/i18n/$defs/TranslatableMixin"
          ]
        },
        "Fixture": {
          "type": "object",
          "primaryKeys": [
            "canonicalName"
          ],
          "extends": [
            "https://numengo.org/ngoschema#/$defs/instances/$defs/Entity"
          ],
          "properties": {
            "model": {
              "$ref": "#/$defs/app/$defs/Model"
            },
            "pk": true,
            "fields": {
              "type": "object"
            }
          }
        },
        "View": {
          "type": "object",
          "extends": [
            "https://numengo.org/ngoschema#/$defs/protocols/$defs/Serializer",
            "#/$defs/i18n/$defs/TranslatableMixin"
          ]
        },
        "App": {
          "type": "object",
          "primaryKeys": [
            "canonicalName"
          ],
          "extends": [
            "https://numengo.org/ngoschema#/$defs/instances/$defs/Entity"
          ],
          "properties": {
            "models": {
              "type": "array",
              "items": {
                "$ref": "#/$defs/app/$defs/Model"
              }
            },
            "fixtures": {
              "type": "array",
              "items": {
                "$ref": "#/$defs/app/$defs/Fixture"
              }
            },
            "views": {
              "type": "array",
              "items": {
                "$ref": "#/$defs/app/$defs/View"
              }
            },
            "entry_points": {
              "type": "array"
            }
          }
        },
        "Telebot": {
          "type": "object",
          "notSerialized": [
            "token"
          ],
          "primaryKeys": [
            "telebot_name"
          ],
          "required": [
            "token",
            "telebot_name"
          ],
          "extends": [
            "https://numengo.org/ngoschema#/$defs/instances/$defs/Entity"
          ],
          "properties": {
            "telebot_name": {
              "type": "string"
            },
            "token": {
              "type": "string"
            },
            "threaded": {
              "type": "boolean"
            },
            "num_threads": {
              "type": "integer",
              "default": 2
            }
          }
        },
        "Chatbot": {
          "type": "object",
          "abstract": true,
          "primaryKeys": [
            "chatbot_name"
          ],
          "extends": [
            "https://numengo.org/ngoschema#/$defs/instances/$defs/Entity"
          ],
          "properties": {
            "chatbot_name": {
              "type": "string"
            },
            "responses": {
              "type": "array"
            }
          }
        }
      }
    }
  }
}